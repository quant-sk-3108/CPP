#include <iostream>
using namespace std;

bool isPalindrome(int n) {
    int rev = 0, temp = n;
    while (temp > 0) {
        rev = rev * 10 + temp % 10;
        temp /= 10;
    }
    return rev == n;
}

int main() {
    int maxPalindrome = 0;

    for (int i = 999; i >= 100; i--) {
        for (int j = i; j >= 100; j--) {
            int product = i * j;
            if (product <= maxPalindrome) break;

            if (isPalindrome(product)) {
                maxPalindrome = product;
            }
        }
    }

    cout << maxPalindrome << endl;
    return 0;
}
