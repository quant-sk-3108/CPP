#include <iostream>
using namespace std;

// Function to compute GCD using Euclidean Algorithm
long long gcd(long long a, long long b) {
    while (b != 0) {
        long long temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}

// Function to compute LCM
long long lcm(long long a, long long b) {
    return (a / gcd(a, b)) * b;
}

int main() {
    long long result = 1;

    for (int i = 1; i <= 20; i++) {
        result = lcm(result, i);
    }

    cout << result << endl;
    return 0;
}
