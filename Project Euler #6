#include <iostream>
using namespace std;

int main() {
    long long n = 100;

    long long sum = n * (n + 1) / 2;
    long long sumOfSquares = n * (n + 1) * (2 * n + 1) / 6;

    long long squareOfSum = sum * sum;
    long long difference = squareOfSum - sumOfSquares;

    cout << difference << endl;
    return 0;
}
